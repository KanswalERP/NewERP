                    @{
                        if (ERPContext.UserContext != null)
                        {
                            int roleId = 0;
                            roleId = ERPContext.UserContext.UserRoles[0].RoleId;


                            if (roleId == 1)
                            {
                                Html.RenderPartial("_LeftMenuNewAffiliation");
                            }
                            else if (roleId == 5) // Researcher          
                            {
                                Html.RenderPartial("_LeftMenuNewResearcher");
                            }
                            else if (roleId == 3) // affiliation Committee          
                            {
                                Html.RenderPartial("_LeftMenuInspectionCommittee");
                            }
                            else if ( roleId == 2 || roleId == 4 || (roleId >= 6 &&  roleId <=11) || roleId == 15 || roleId == 16 || roleId == 17 )
                            {
                                Html.RenderPartial("_LeftMenuInspection");
                            }
                            else if (roleId == 12 || roleId == 14 || roleId == 13)
                            {
                                Html.RenderPartial("_LeftMenuCollegeUniversity");
                            }
                            else if (roleId == 18) // Teacher        
                            {
                                Html.RenderPartial("_LeftMenuCollegeUniversityTeacher");
                            }
                        }
                    }
